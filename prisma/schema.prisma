generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  telegramId String   @unique
  username   String?  @unique
  isActive   Boolean  @default(false)
  createdAt  DateTime @default(now())
}
